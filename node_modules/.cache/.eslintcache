1'}>\r\n                <div className={'Name'}>{props.student.name}</div>\r\n                <div className={'Grade'}>{props.student.grade}</div>\r\n                <div></div>\r\n                {/*공백 맞추기용*/}\r\n                <button onClick={handleClick} id={'Arrow1'}><img src={Arrow1} width={'33px'} height={'33px'}/></button>\r\n            </div> :\r\n            <div className={'TableContent2'}>\r\n                <div className={'Name'}>{props.student.name}</div>\r\n                <div className={'Grade'}>{props.student.grade}</div>\r\n                <div></div>\r\n                {/*공백 맞추기용*/}\r\n                <button onClick={handleClick} id={'Arrow2'}><img src={Arrow2} width={'33px'} height={'33px'}/>\r\n                </button>\r\n            </div>\r\n    )\r\n\r\n}\r\n\r\n\r\nexport default TableContent;","C:\\Users\\hdchr\\OneDrive\\Desktop\\waffle-rookies-19.5-react-assignment-2\\src\\component\\Table\\TableHeader\\TableHeader.js",[],"C:\\Users\\hdchr\\OneDrive\\Desktop\\waffle-rookies-19.5-react-assignment-2\\src\\component\\Detail\\DetailHeader\\DetailHeader.js",["141","142","143"],"import './DetailHeader.css'\r\nimport {useStudentContext} from '../../../context/Context';\r\nimport image from '../../../Data/PageMove.svg'\r\nimport {Router, Link} from 'react-router-dom';\r\n\r\nconst DetailHeader = (props) => {\r\n    const context = useStudentContext();\r\n\r\n    return (\r\n        <div className={'DetailHeader'}>\r\n            <div></div>\r\n            <button onClick={() => {\r\n                context.setNowStudentData(context.studentData.filter(student=>student.id===context.nowStudentData.id)[0]);\r\n                props.history.push('/students/:id')\r\n            }} id={'GoDetail'}><img src={image}/></button>\r\n\r\n        </div>\r\n    )\r\n}\r\n\r\n\r\nexport default DetailHeader;\r\n","C:\\Users\\hdchr\\OneDrive\\Desktop\\waffle-rookies-19.5-react-assignment-2\\src\\component\\Detail\\DetailPic\\DetailPic.js",["144"],"import '.