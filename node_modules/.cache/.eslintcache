ext = useStudentContext();\r\n\r\n    return (\r\n        <div className={'DetailHeader'}>\r\n            <div></div>\r\n            <button onClick={() => {\r\n                context.setNowStudentData(context.studentData.filter(student=>student.id===context.nowStudentData.id)[0]);\r\n                props.history.push('/students/:id')\r\n            }} id={'GoDetail'}><img src={image}/></button>\r\n\r\n        </div>\r\n    )\r\n}\r\n\r\n\r\nexport default DetailHeader;\r\n","C:\\Users\\hdchr\\OneDrive\\Desktop\\waffle-rookies-19.5-react-assignment-2\\src\\component\\Detail\\DetailPic\\DetailPic.js",["147"],"import './DetailPic.css'\r\nimport {useStudentContext} from '../../../context/Context';\r\n\r\nconst DetailPic = () => {\r\n    const context = useStudentContext();\r\n    return (\r\n        <div className={'DetailPic'}>\r\n            <img src={context.nowStudentData.profileImg} alt='No Image'\r\n            width = {'100%'} height={'100%'} />\r\n        </div>\r\n    )\r\n}\r\n\r\n\r\nexport default DetailPic\r\n\r\n","C:\\Users\\hdchr\\OneDrive\\Desktop\\waffle-rookies-19.5-react-assignment-2\\src\\component\\Detail\\DetailContent\\DetailContent.js",["148"],"import './DetailContent.css'\r\nimport {useState} from 'react';\r\nimport {useStudentContext} from '../../../context/Context';\r\n\r\nconst DetailContent = () => {\r\n    const context = useStudentContext();\r\n    return (\r\n        <ul className={'DetailContent'}>\r\n            <li className={'Line'}>\r\n                이름 <input disabled={true} className={'DetailInput'} value={context.nowStudentData.name}/>\r\n            </li>\r\n            <li className={'Line'}>\r\n                학년 <input disabled={true} className={'DetailInput'} value={context.nowStudentData.grade}/>\r\n            </li>\r\n        </ul>\r\n    )\r\n}\r\n\r\nexport default DetailContent","C:\\Users\\hdchr\\OneDrive\\Desktop\\waffle-rookies-19.5-react-assignment-2\\src\\component\\Table\\NoTableContents\\NoTableContents.js",